/* fish eye footer*/
var FEfooter =3D Class.create();
FEfooter.prototype =3D=20
{
	initialize: function(elmId, JSONurl)
	{
		// init parameters
		this.container =3D $(elmId);
		this.JSONurl =3D JSONurl;
		this.init =3D true;
		this.inProcess =3D false;
		this.smallImgWidth =3D 116;
		this.bigImgWidth =3D 139;
		this.smallImgHeight =3D 67;
		this.bigImgHeight =3D 80;
		this.smallDivWidth =3D 145;
		this.bigDivWidth =3D 167;
	=09
		this.waitMouseOver =3D 10;
		this.timeoutObj =3D null;
	=09
		this.delay =3D 0.3;
	=09
		this.elms =3D [];
	=09
		// Get JSON data feed
		this.getData();
	},

	getData: function()
	{
		var oAJAX =3D new Ajax.Request(this.JSONurl,=20
		{
			onSuccess: this.processData.bind(this),
			onFailure: function()
			{
			},
			method: 'get'
		});
		=09
	},
	=09
	processData: function(xhr)
	{
		var template =3D '';
		var data =3D xhr.responseText.evalJSON();
		if(data.items.length!=3D5) return;	=09
		=09
		// Get the HTML template
		var templateLoc =3D this.container.select('ul li.JSONize')[0];
		var JSONtemplate =3D templateLoc.innerHTML;
		templateLoc.remove();
		var content=3D null;
		var $target =3D this.container.select('ul')[0];
		// merge content with structure
	=09
		var item;
	=09
		for(i=3D0;(item=3Ddata.items[i]);i++)
		{
			if (item.link.href) {
				content =3D JSONtemplate.interpolate({
					title: item.title,
					link_title: item.title,
					link_href: 'href=3D"' + item.link.href + '"',
					link_target: 'target=3D"' + item.link.target + '"',
					img_alt: item.img.alt,
					img_src: 'src=3D"' + item.img.src + '"'
				});
			}
			else
			{
				content =3D JSONtemplate.interpolate({
					title: item.title,
					link_title: item.title,
					link_href: 'href=3D"' + item.link+ '"',
					link_target: 'target=3D"_self"',
					img_alt: item.img.alt,
					img_src: 'src=3D"' + item.img.src + '"'
				});
			}
		=09
			var newLI =3D new Element('li').update(content);
		=09
			if(i=3D=3D0) newLI.addClassName('first');

			$target.insert(newLI);		=09
		}
			=09
		this.items =3D $target.select('li');
	=09
		this.items.each(
	=09
			function($item, index)
			{
				$item.observe('mouseenter', function()
				{
					if(!$item.hasClassName('big'))
					{
						this.collapse(index);
					}								=09
				}.bind(this));
			=09
			=09
				//$item.setStyle({position: 'relative'});
				var $img =3D $item.select('img')[0];
				var $Anchor =3D $item.select('a')[0];
			=09
				$img.observe('click', function(){
					if($Anchor.target.indexOf("_blank") > -1)
						window.open($Anchor.href);=20
					else
						location.href =3D $Anchor.href
				});
			}.bind(this)
		);
		=09
		// enlarge main image=20
		this.collapse(2);
	},
=09
	collapse: function(index)
	{
		if(this.inProcess) return;
		//get the right elements
		// enlarge main image
		this.elms[0] =3D [this.items[index]];
		this.resize(0);
	=09
		var lIndex =3D index+1;
		var rIndex =3D index-1;
	=09
		var rItem=3Dthis.items[rIndex];
		var lItem=3Dthis.items[lIndex];	=09
							=09
		for(var depth=3D0;(rItem || lItem);)
		{
			this.elms[++depth] =3D [];
			if (rItem)=20
			{			=09
				this.elms[depth].push(rItem);
			}
			if(lItem)
			{=09
				this.elms[depth].push(lItem);
			}=09
					=09
			if(this.elms[depth].length > 0) this.resize(depth);
			rItem=3Dthis.items[--rIndex];
			lItem=3Dthis.items[++lIndex];		=09
		}
	=09
	=09

	=09
		this.init =3D false;			=09
	},
=09
	resize: function(_depth)
	{	=09
		var _elms =3D this.elms[_depth];

		var $img =3D [];
		var resizeImgW =3D [];
		var resizeImgH =3D [];	=09
	=09
		var resizeItem =3D [];
		var posItem =3D [];
	=09
		var pResize =3D new Parallel();
			=09
		for(var j=3D0;_elms[j];j++)
		{=09
			var $elm =3D _elms[j];=20
			$elm.setStyle({zIndex:5-_depth});=09
		=09
			// if resize needed
			=09
			var oldIMGwidth, newIMGwidth, oldDivWidth, newDivWidth;
			$img[j] =3D $elm.select('img')[0];
		=09
			oldIMGwidth =3D parseInt($img[j].getStyle('width'))*1;
			oldIMGheight =3D parseInt($img[j].getStyle('height'))*1;

			if (_depth =3D=3D 0)=20
			{
				newIMGwidth =3D this.bigImgWidth;
				//oldIMGheight =3D this.smallImgHeight;
				newIMGheight =3D this.bigImgHeight;
				//oldDivWidth =3D this.smallDivWidth;
				newDivWidth =3D this.bigDivWidth;
			}
			else=20
			{
				newIMGwidth =3D this.smallImgWidth;
				//oldIMGheight =3D this.bigImgHeight;
				newIMGheight =3D this.smallImgHeight;
				newDivWidth =3D this.smallDivWidth;
			}
			=09
			$img[j] =3D $elm.select('img')[0];

			if(Brsr=3D=3D'IE')
			{
				oldDivWidth =3D parseInt($elm.getStyle('width'))*1;
				resizeItem[j] =3D new Tween($elm.style, 'width', Tween.regularEaseOut,=
 oldDivWidth, newDivWidth, this.delay, 'px');
				pResize.addChild(resizeItem[j]);=09
			}
		=09
			resizeImgW[j] =3D new Tween($img[j].style, 'width', Tween.None, oldIMGwi=
dth, newIMGwidth, this.delay, 'px');

			resizeImgH[j] =3D new Tween($img[j].style, 'height', Tween.None, oldIMGh=
eight, newIMGheight, this.delay, 'px');
		=09
		=09
			if(_depth=3D=3D0)
			{	=09
				resizeImgW[j].onMotionFinished =3D function()		=09
				{
					this.addClassName('big');=09
				}.bind($elm);
			}
			else
			{
				resizeImgW[j].onMotionStarted =3D function()		=09
				{
					this.removeClassName('big');
				}.bind($elm);							=09
			}
			pResize.addChild(resizeImgW[j]);
			pResize.addChild(resizeImgH[j]);
		=09
			var curPos =3D parseInt($elm.getStyle('margin-top')) * 1;
			var marginOffset =3D (_depth > 0) ? (10 + _depth) : 0;
			posItem[j] =3D new Tween($elm.style, 'marginTop', Tween.regularEaseOut,c=
urPos,marginOffset, this.delay, 'px');
		=09
			pResize.addChild(posItem[j]);
		=09
			pResize.onMotionStarted =3D function()		=09
			{
				this.inProcess =3D true;
			}.bind(this);		=09
		=09
			pResize.onMotionFinished =3D function()		=09
			{
				this.inProcess =3D false;
			}.bind(this);			=09
		}	=09

		pResize.start();
	}
}

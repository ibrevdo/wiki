        	'Nasdaq': '=D7=A0=D7=A1=D7=93&#34;=D7=A7'
		},
		options:{		=09
			speed:800,
			easing:"easeInQuad"
		},=20
		_create:function(){
			var self=3Dthis, o=3Dthis.options, el=3Dthis.element;
			self.container =3D el.children('dl').eq(0);
			var $buttons =3D self.container.children('dt');
			$buttons.eq(0).css("border-top-width","0px");
			self.$btn=3Dnull;
			/* add open capability to tab headers */
			$buttons.each(function(index)
			{
				var $btn=3D$(this);
				var $content =3D $btn.next('dd');
				if(!$content[0]) return true;
				$btn.click(function(){
					if(!(self.container.find('dd:animated').length))
					{
						if($content.is(':visible')){
							self._close($btn);
						}
						else
						{
							if(self.$btn !=3Dnull && self.$btn !=3D$btn && self.$btn.next('dd').=
is(':visible'))
							{
								self._close(self.$btn,$btn);
							}
							else {
								self._open($btn);
							}
						}
					}
				});	=09
				var $UL =3D $content.children('ul').eq(0);
				if(!$UL.hasClass('JSON'))
					$content.height($UL.outerHeight(true));	=09
				$content.hide();
			});
			self.container.find('dd ul li').hover(function(){
				$(this).addClass("hover");
			},
			function(){
				$(this).removeClass("hover");
			});
			// add submit button special behaviour
			var $submit =3D self.container.find('dd ul li a.submit');
			$submit.click(function(e){
				e.preventDefault();
				var $Btn=3D$(this);
				var val =3D $Btn.parent().prev().find('input[type=3Dtext]').eq(0).val();
				var newHref =3D $Btn.attr('href') + encodeURIComponent(val);
				if (val !=3D '') {
					if ($Btn.attr('target') =3D=3D '_blank') {
						var newWindow =3D window.open(newHref, '_blank');
						newWindow.focus();
					}
					else {
						window.location.href =3D newHref;
					}
				}
